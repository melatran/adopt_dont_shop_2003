<h1>All Pets</h1>

<% @pets.each do |pet| %>
  <img src="<%= pet.image %>" alt-"it could be yours" width = "300" height = "300"</img>
  <p>Name:<%= pet.name %></p>
  <p>Approximate Age:<%= pet.approximate_age %></p>
  <p>Sex:<%= pet.sex %></p>
  <p>Current Location:<%= pet.name_of_shelter_where_pet_is_currently_located %></p>
<% end %>
